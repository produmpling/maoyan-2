page {
  background-color: #f5f5f5;
}

.warp {
  background-color: #fff;
  margin-top: 20rpx;
  font-size: 28rpx;
  border-bottom: 1rpx #dcdcdc solid;
  border-top: 1rpx #dcdcdc solid;
}

.item {
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
}
.navigator {
  padding-left: 30rpx;
}
.item::after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #ccc;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  right: 0;
  top: 36rpx;
  margin-right: 28rpx;
  margin-top: 0rpx;
}
.last{
    border-bottom:none;
}
.login {
  width: 100%;
  height: 100vh;
  position: fixed;
  opacity: 0;
  left: 0;
  top: 0;
  z-index: 11;
}
.user-avatar {
  width:200rpx;
  height:200rpx;
  overflow:hidden;
  border-radius:50%;
  -webkit-border-radius:50%;
  -moz-border-radius:50%;
  -ms-border-radius:50%;
  -o-border-radius:50%;
  margin:60rpx auto
}